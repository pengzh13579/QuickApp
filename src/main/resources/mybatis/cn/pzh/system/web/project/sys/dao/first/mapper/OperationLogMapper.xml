<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.pzh.system.web.project.sys.dao.mapper.OperationLogMapper">

    <resultMap id="BaseResultMap" type="cn.pzh.system.web.project.common.dao.first.entity.SystemOperationLogEntity" >
        <result column="ID" property="id" jdbcType="INTEGER" />
        <result column="LOG_NAME" property="logName" jdbcType="VARCHAR" />
        <result column="CREATE_USER" property="createUser" jdbcType="VARCHAR" />
        <result column="CREATE_DATE" property="createDate" jdbcType="DATE" />
        <result column="SUCCEED" property="succeed" jdbcType="VARCHAR" />
        <result column="MESSAGE" property="message" jdbcType="VARCHAR" />
        <result column="IP" property="ip" jdbcType="VARCHAR" />
    </resultMap>

    <insert id="saveLoginLog" parameterType="cn.pzh.system.web.project.common.dao.first.entity.SystemLoginLogEntity" useGeneratedKeys="true" keyProperty="id">
        insert into sys_login_log (
            LOG_NAME,
            CREATE_USER,
            CREATE_DATE,
            SUCCEED,
            MESSAGE,
            IP)
        values (
            #{logName,jdbcType=VARCHAR},
            #{createUser,jdbcType=VARCHAR},
            #{createDate,jdbcType=VARCHAR},
            #{succeed,jdbcType=NUMERIC},
            #{message,jdbcType=VARCHAR},
            #{ip,jdbcType=TIMESTAMP}
        )
    </insert>

</mapper>
